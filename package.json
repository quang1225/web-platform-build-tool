{
  "name": "web-platform-build-tool",
  "version": "0.2.8",
  "license": "MIT",
  "author": "quang1225",
  "email": "quang1225@gmail.com",
  "description": "This is a build tool CLI for simplifying and streamlining the build process for internal frontend projects in the Grab Taxi company, speed up build time and optimize performance. Support the Micro Frontend and make the dev experience smoother.",
  "bin": {
    "web-platform-build-tool": "./dist/cli.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "publish:dev": "yarn run clean && yarn build && npm version prerelease --preid=dev && npm publish --tag dev",
    "publish:beta": "yarn run clean && yarn build && npm version prerelease --preid=beta && npm publish --tag beta",
    "publish:release": "yarn run clean && yarn build && npm version patch && npm publish",
    "publish:minor": "yarn run clean && yarn build && npm version minor && npm publish",
    "publish:major": "yarn run clean && yarn build && npm version major && npm publish",
    "publish:latest": "yarn run clean && yarn build && npm version patch && npm publish"
  },
